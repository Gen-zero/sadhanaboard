{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.sadhanaboard.com/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://api.sadhanaboard.com/socket.io/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.js$",
      "headers": {
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.css$",
      "headers": {
        "content-type": "text/css"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.png$",
      "headers": {
        "content-type": "image/png"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.jpg$",
      "headers": {
        "content-type": "image/jpeg"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.jpeg$",
      "headers": {
        "content-type": "image/jpeg"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.webp$",
      "headers": {
        "content-type": "image/webp"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.svg$",
      "headers": {
        "content-type": "image/svg+xml"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.ico$",
      "headers": {
        "content-type": "image/x-icon"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "nodeVersion": "22.x"
}