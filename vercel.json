{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.sadhanaboard.com/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://api.sadhanaboard.com/socket.io/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}