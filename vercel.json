{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.sadhanaboard.com/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://api.sadhanaboard.com/socket.io/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.js$",
      "headers": {
        "content-type": "text/javascript; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.css$",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "/assets/index-D82wIXYJ.css",
      "dest": "/assets/index-Cb-XSSXK.css",
      "statusCode": 301
    },
    {
      "src": "/index-SrtPg-iy.js",
      "dest": "/assets/index-oYaPtp-r.js",
      "statusCode": 301
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}